<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/_f7f7f7"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/_7dp"
        android:background="@drawable/white_val"
        android:orientation="vertical"
        android:padding="@dimen/_7dp" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/_00cc99"
                android:textSize="@dimen/_14sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/_666666"
                android:textSize="@dimen/_12sp" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1px"
            android:background="@color/_e5e5e5" />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1px"
            android:layout_marginTop="@dimen/_5dp"
            android:background="@color/_e5e5e5" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_30dp"
            android:orientation="horizontal" >

            <View
                android:layout_width="@dimen/_1px"
                android:layout_height="match_parent"
                android:background="@color/_e5e5e5" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/ws"
                android:textColor="@color/_333333"
                android:textSize="@dimen/_12sp"
                android:textStyle="bold" />

            <View
                android:layout_width="@dimen/_1px"
                android:layout_height="match_parent"
                android:background="@color/_e5e5e5" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/ly"
                android:textColor="@color/_333333"
                android:textSize="@dimen/_12sp"
                android:textStyle="bold" />

            <View
                android:layout_width="@dimen/_1px"
                android:layout_height="match_parent"
                android:background="@color/_e5e5e5" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/yc"
                android:textColor="@color/_333333"
                android:textSize="@dimen/_12sp"
                android:textStyle="bold" />

            <View
                android:layout_width="@dimen/_1px"
                android:layout_height="match_parent"
                android:background="@color/_e5e5e5" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1px"
            android:background="@color/_e5e5e5" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_30dp"
            android:orientation="horizontal" >

            <View
                android:layout_width="@dimen/_1px"
                android:layout_height="match_parent"
                android:background="@color/_e5e5e5" />

            <ImageView
                android:id="@+id/ws"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:contentDescription="@string/app_name"
                android:padding="@dimen/_3dp"
                android:scaleType="centerInside" />

            <View
                android:layout_width="@dimen/_1px"
                android:layout_height="match_parent"
                android:background="@color/_e5e5e5" />

            <ImageView
                android:id="@+id/ly"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:contentDescription="@string/app_name"
                android:padding="@dimen/_3dp"
                android:scaleType="centerInside" />

            <View
                android:layout_width="@dimen/_1px"
                android:layout_height="match_parent"
                android:background="@color/_e5e5e5" />

            <ImageView
                android:id="@+id/yc"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:contentDescription="@string/app_name"
                android:padding="@dimen/_3dp"
                android:scaleType="centerInside" />

            <View
                android:layout_width="@dimen/_1px"
                android:layout_height="match_parent"
                android:background="@color/_e5e5e5" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1px"
            android:background="@color/_e5e5e5" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_30dp"
            android:orientation="horizontal" >

            <View
                android:layout_width="@dimen/_1px"
                android:layout_height="match_parent"
                android:background="@color/_e5e5e5" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/jl"
                android:textColor="@color/_333333"
                android:textSize="@dimen/_12sp"
                android:textStyle="bold" />

            <View
                android:layout_width="@dimen/_1px"
                android:layout_height="match_parent"
                android:background="@color/_e5e5e5" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/fy"
                android:textColor="@color/_333333"
                android:textSize="@dimen/_12sp"
                android:textStyle="bold" />

            <View
                android:layout_width="@dimen/_1px"
                android:layout_height="match_parent"
                android:background="@color/_e5e5e5" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/kq"
                android:textColor="@color/_333333"
                android:textSize="@dimen/_12sp"
                android:textStyle="bold" />

            <View
                android:layout_width="@dimen/_1px"
                android:layout_height="match_parent"
                android:background="@color/_e5e5e5" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1px"
            android:background="@color/_e5e5e5" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_30dp"
            android:orientation="horizontal" >

            <View
                android:layout_width="@dimen/_1px"
                android:layout_height="match_parent"
                android:background="@color/_e5e5e5" />

            <ImageView
                android:id="@+id/jl"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:contentDescription="@string/app_name"
                android:padding="@dimen/_3dp"
                android:scaleType="centerInside" />

            <View
                android:layout_width="@dimen/_1px"
                android:layout_height="match_parent"
                android:background="@color/_e5e5e5" />

            <ImageView
                android:id="@+id/fy"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:contentDescription="@string/app_name"
                android:padding="@dimen/_3dp"
                android:scaleType="centerInside" />

            <View
                android:layout_width="@dimen/_1px"
                android:layout_height="match_parent"
                android:background="@color/_e5e5e5" />

            <ImageView
                android:id="@+id/kq"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:contentDescription="@string/app_name"
                android:padding="@dimen/_3dp"
                android:scaleType="centerInside" />

            <View
                android:layout_width="@dimen/_1px"
                android:layout_height="match_parent"
                android:background="@color/_e5e5e5" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1px"
            android:background="@color/_e5e5e5" />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1px"
            android:layout_marginTop="@dimen/_7dp"
            android:background="@color/_e5e5e5" />

        <TextView
            android:id="@+id/class_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5dp"
            android:textColor="@color/_666666"
            android:textSize="@dimen/_10sp"
            android:textStyle="bold" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_1px"
        android:layout_marginTop="@dimen/_5dp"
        android:background="@color/_e5e5e5" />

</LinearLayout>